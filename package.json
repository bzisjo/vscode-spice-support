{
    "name": "vscode-spice-support",
    "displayName": "SPICE - Language Support",
    "description": "Language support for various flavors of SPICE",
    "version": "0.0.4",
    "publisher": "bzisjo",
    "author": {
        "name": "Jonathan Wang",
        "email": "bzisjo@gmail.com"
    },
    "engines": {
        "vscode": "^1.33.0"
    },
    "license": "MIT",
    "keywords": [
        "spice",
        "syntax",
        "highlighting",
        "circuit",
        "snippets"
    ],
    "icon": "icon.gif",
    "repository": {
        "type": "git",
        "url": "https://github.com/bzisjo/vscode-spice-support"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "spice",
                "aliases": [
                    "SPICE",
                    "spice"
                ],
                "extensions": [
                    ".ckt",
                    ".sp",
                    ".net",
                    ".cir",
                    ".cirx",
                    ".mod",
                    ".mdl",
                    ".lib",
                    ".sub",
                    ".scs",
                    ".inc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "spice",
                "scopeName": "source.sp",
                "path": "./syntaxes/SPICE.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "spice",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "spice",
                "path": "./snippets/snippets_meas.json"
            }
        ]
    }
}
